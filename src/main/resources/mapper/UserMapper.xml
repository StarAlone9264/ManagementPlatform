<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.deer.managementPlatform.dao.UserMapper">

    <select id="queryByName" parameterType="java.lang.String" resultType="com.deer.managementPlatform.entity.User">
        select * from SKY.TB_USER where USERNAME = #{username}
    </select>
</mapper>